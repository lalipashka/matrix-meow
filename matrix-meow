matrix_name = "queen cat-Meow"
# Запрашиваем у пользователя ввод элементов матрицы
matrix = []
for i in range(3):
    row = [int(x) for x in input(f"Введите элементы {i + 1}-й строки через пробел: ").split()]
    matrix.append(row)

# Создаем копию матрицы и называем ее copyCat
copyCat = [row[:] for row in matrix]
copyCat = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
# Выводим элементы оригинальной матрицы
print(f"{matrix_name} (оригинальная матрица):")
for row in matrix:
    print(' '.join(map(str, row)))


while True:
    command = input("Введите команду ('введите 'проверка на наличие вкусняшек' для проверки нужных чисел в матрице', 'введите 'съела весь корм' для удаления' или 'введите 'meow-meow' для выхода'): ")

    if command == 'meow-meow':
        break
    elif command == 'съела весь корм':
        matrix.clear()
        copyCat.clear()
        print("Матрица и ее копия удалены.")
    elif command == 'проверка на наличие вкусняшек':
        element_to_check = input("Введите элемент для проверки наличия в матрице: ")

        found_in_original = any(element_to_check.isdigit() and int(element_to_check) in row for row in matrix)
        found_in_copy = any(element_to_check.isdigit() and int(element_to_check) in row for row in copyCat)

        if found_in_original:
            print(f"Элемент {element_to_check} найден в queen cat-Meow.")
        else:
            print(f"Элемент {element_to_check} не найден в queen cat-Meow.")

        if found_in_copy:
            print(f"Элемент {element_to_check} найден в copyCat.")
        else:
            print(f"Элемент {element_to_check} не найден в copyCat.")
    else:
        print("Некорректная команда. Попробуйте снова.")
